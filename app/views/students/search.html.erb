
<div class="container-fluid">
                       <div style="height: 1vh;"></div>
                        <ol class="breadcrumb mb-4">
                            <li class="breadcrumb-item"><a href="index.html">Dashboard</a></li>
                            <li class="breadcrumb-item active">Search Student</li>
                        </ol>
                        <div class="card mb-4">
                            <div class="card-body">
                              <%= form_tag search_path, method: :get do %>
                           <div class='form-group row'>
                             <div class="col-sm-9">
                               <%= text_field_tag :name, params[:name], id: 'symbols', class:'form-control form-control-lg' %>


                             </div>
                             <div class="col-sm-3">
                               <%= button_tag type: :submit , class: 'btn btn-success' do %>
                               <%= 'Search' %>
                               <% end %>
                             </div>
                                 <% end %>
                           </div>
                            </div>
                        </div>
                        <div style="height: 4vh;"></div>

                        <div class="card mb-4">

                          <div class="card-body">


                            <%  if @student %>


                            <ol class="breadcrumb mb-4">
                                <li class="breadcrumb-item"><a href="index.html">Search Result</a></li>

                            </ol>


                            <table class="table">
                              <thead class="thead-dark">
                                <tr>


                                  <th scope="col">Property</th>
                                  <th scope="col">Value</th>
                                </tr>
                              </thead>
                              <tbody>
                                <tr>

                                  <td>Name </td>
                                  <td><%= @student.name %></td>
                                </tr>
                                <tr>


                                  <td>Caste</td>
                                  <td><%= @student.caste %></td>
                                </tr>
                                <tr>

                                  <td>Admission Year</td>
                                  <td><%= @student.admission_year %></td>
                                </tr>

                                <tr>

                                  <td>academic_year</td>
                                  <td><%= @student.academic_year %></td>
                                </tr>
                                <tr>

                                  <td>student_class</td>
                                  <td><%= @student.student_class %></td>
                                </tr>
                                <tr>

                                  <td>branch</td>
                                  <td><%= @student.branch %></td>
                                </tr>
                                <tr>

                                  <td>application_id</td>
                                  <td><%= @student.application_id %></td>
                                </tr>

                                <tr>

                                  <td>tution_fee</td>
                                  <td><%= @student.tution_fee %></td>
                                </tr>


                                <tr>

                                  <td>devlopment_fee</td>
                                  <td><%= @student.devlopment_fee %></td>
                                </tr>

                                <tr>

                                  <td>other_fee</td>
                                  <td><%= @student.other_fee %></td>
                                </tr>

                                <tr>

                                  <td>total</td>
                                  <td><%= @student.total %></td>
                                </tr>
                                <tr>


                              </tbody>
                            </table>











                            <% end %>


                          </div>

                        </div>
</div>






















<script>
  $( function() {
    var availableTags =  <%=raw @names  %>;
    $( "#symbols" ).autocomplete({
      source: availableTags
    });
  } );
</script>





 
